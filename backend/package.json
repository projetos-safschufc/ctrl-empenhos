{
  "name": "app-ctrl-emp-estq",
  "version": "1.0.0",
  "description": "Sistema de Controle de Empenhos e Estoque",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "node scripts/migrate-with-env.js dev",
    "prisma:migrate:deploy": "node scripts/migrate-with-env.js deploy",
    "prisma:studio": "prisma studio",
    "seed": "ts-node prisma/seed.ts",
    "db:init": "node scripts/run-init-ctrl-schema.js",
    "db:fix-users-columns": "node scripts/run-fix-users-columns.js",
    "db:fix-catalogo-columns": "node scripts/run-fix-catalogo-columns.js",
    "db:fix-hist-empenho-columns": "node scripts/run-fix-hist-empenho-columns.js",
    "db:fix-empenho-columns": "node scripts/run-fix-empenho-columns.js",
    "db:optimize": "node database/optimizations/apply_indexes.js",
    "db:analytics": "node database/analytics/apply_analytics.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "pdfkit": "^0.15.0",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.9.0",
    "@types/pdfkit": "^0.13.3",
    "@types/pg": "^8.11.6",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
